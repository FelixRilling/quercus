{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./README.md", "./dist/quercus.esm.js"],
        "excludePattern": "(node_modules/|docs/)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "template": "./node_modules/countly-docdash",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true
    }
}
