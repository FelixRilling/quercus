{
    "name": "quercus",
    "version": "v1.0.0",
    "description": "Simple and fast tree data structures in JS",
    "main": "dist/quercus.common.js",
    "module": "dist/quercus.esm.js",
    "files": ["src/", "dist/"],
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main.ts",
            "namespace": {
                "module": "Quercus",
                "file": "quercus"
            },
            "targets": {
                "browsers": ["Chrome >= 58"]
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build test docs",
        "build": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "test": "jasmine",
        "lint": "tslint src/",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/quercus.git"
    },
    "keywords": ["tree", "data-structure"],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/quercus/issues"
    },
    "homepage": "https://github.com/FelixRilling/quercus#readme",
    "dependencies": {
        "lightdash": "^3.3.2"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "docdash": "git+https://github.com/clenemt/docdash.git",
        "jasmine": "^2.8.0",
        "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
        "npm-run-all": "^4.1.2",
        "rollup": "^0.52.3",
        "rollup-plugin-babel": "^3.0.3",
        "rollup-plugin-typescript2": "^0.9.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.2",
        "uglify-es": "^3.2.2"
    }
}
