{
    "name": "quercus",
    "version": "2.1.0",
    "description": "Simple and fast tree data structures in JS",
    "main": "dist/quercus.common.js",
    "module": "dist/quercus.esm.js",
    "types": "types/main.d.ts",
    "settings": {
        "input": "main.ts",
        "namespace": {
            "module": "Quercus",
            "file": "quercus"
        },
        "external": ["lightdash"]
    },
    "scripts": {
        "start": "npm-run-all lint build test docs",
        "build": "rollup -c ./tasks/rollup.config.js",
        "build:iife": "rollup -c ./tasks/rollup.config.iife.js",
        "build:iife-min": "rollup -c ./tasks/rollup.config.iife-min.js",
        "watch": "rollup -c ./tasks/rollup.config.js --watch",
        "dist": "npm-run-all --parallel build build:iife build:iife-min",
        "lint": "eslint src/",
        "test": "jasmine",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/quercus.git"
    },
    "keywords": ["tree", "data-structure"],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/quercus/issues"
    },
    "homepage": "https://github.com/FelixRilling/quercus#readme",
    "dependencies": {
        "lightdash": "^4.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.38",
        "@babel/plugin-external-helpers": "^7.0.0-beta.38",
        "@babel/preset-env": "^7.0.0-beta.38",
        "docdash": "git+https://github.com/clenemt/docdash.git",
        "eslint": "^4.15.0",
        "jasmine": "^2.8.0",
        "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
        "npm-run-all": "^4.1.2",
        "rollup": "^0.54.1",
        "rollup-plugin-babel": "^4.0.0-beta.0",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-node-resolve": "^3.0.2",
        "rollup-plugin-typescript2": "^0.10.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "typescript": "^2.6.2",
        "typescript-eslint-parser": "^12.0.0",
        "uglify-es": "^3.3.7"
    }
}
